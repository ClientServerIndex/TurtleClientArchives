{
"format_version":"1.8.0",
"render_controllers":{
"controller.render.player.first_person":{
"geometry":"Geometry.default",
"materials":[{"*":"Material.default"}],
"textures":["Texture.default"],
"light_color_multiplier":1,
"ignore_lighting":true,
"part_visibility":[
{"*":false},
{"rightArm":"q.get_equipped_item_name(0,1)==''||q.get_equipped_item_name(0,1)=='map'"},
{"rightSleeve":"q.get_equipped_item_name(0,1)==''||q.get_equipped_item_name(0,1)=='map'"},
{"leftArm":"(q.get_equipped_item_name(0,1)=='map'&&q.get_equipped_item_name('off_hand')!='shield')||(q.get_equipped_item_name('off_hand')=='map'&&!q.item_is_charged)||(!q.item_is_charged&&(v.item_use_normalized>0&&v.item_use_normalized<1.0))"},
{"leftSleeve":"(q.get_equipped_item_name(0,1)=='map'&&q.get_equipped_item_name('off_hand')!='shield')||(q.get_equipped_item_name('off_hand')=='map'&&!q.item_is_charged)||(!q.item_is_charged&&(v.item_use_normalized>0&&v.item_use_normalized<1.0))"}
]
},
"controller.render.player.third_person":{
"geometry":"Geometry.default",
"materials":[{"*":"Material.default"}],
"textures":["Texture.default"],
"light_color_multiplier":1,
"ignore_lighting":true,
"part_visibility":[
{"*":true},
{"helmet":"v.helmet_layer_visible"},
{"leftLegging":"v.leg_layer_visible"},
{"rightLegging":"v.leg_layer_visible"},
{"leftBoot":"v.boot_layer_visible"},
{"rightBoot":"v.boot_layer_visible"},
{"leftSock":"v.boot_layer_visible&&v.leg_layer_visible"},
{"rightSock":"v.boot_layer_visible&&v.leg_layer_visible"},
{"bodyArmor":"v.chest_layer_visible&&!q.has_cape"},
{"leftArmArmor":"v.chest_layer_visible&&!q.has_cape"},
{"rightArmArmor":"v.chest_layer_visible&&!q.has_cape"},
{"belt":"v.chest_layer_visible&&v.leg_layer_visible"}
]
},
"controller.render.freelook":{
